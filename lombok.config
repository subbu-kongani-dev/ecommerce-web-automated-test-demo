# Lombok Configuration
# This helps Eclipse's Lombok plugin process annotations correctly

# Enable builder pattern
lombok.builder.flagUsage = allow

# Enable singular collections
lombok.singular.auto = true

# Enable data annotation
lombok.data.flagUsage = allow

# Copy annotations to generated code
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Qualifier
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Value

# Add generated annotation
lombok.addLombokGeneratedAnnotation = true

# Generate setters return this
config.stopBubbling = true
